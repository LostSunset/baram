    <xs:element name="volumeMonitor">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string" default="volumeAverage"/>
                <xs:element name="reportType">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="volumeAverage"/>
                            <xs:enumeration value="volumeIntegral"/>
                            <xs:enumeration value="minimum"/>
                            <xs:enumeration value="maximum"/>
                            <xs:enumeration value="cov"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="field" type="scalarFieldType"/>
                <xs:element name="volume" type="xs:nonNegativeInteger"/>   <!-- cellZone czid -->
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- root element "configuration" -->

    <xs:element name="configuration">
        <xs:complexType>
            <xs:sequence>

                <xs:element name="general" type="generalType"/>

                <xs:element name="referenceValues" type="referenceValuesType"/>

                <xs:element name="materials">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="material" type="materialType" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="models">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="multiphaseModels" type="multiphaseModelsType"/>
                            <xs:element name="energyModels">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="off"/>
                                        <xs:enumeration value="on"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="turbulenceModels" type="turbulenceModelsType"/>
                            <xs:element name="speciesModels">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="off"/>
                                        <xs:enumeration value="on"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="regions">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="region" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="name" type="xs:string"/>
                                        <xs:element name="material" type="xs:positiveInteger"/> <!-- material mid -->
                                        <xs:element name="cellZones">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element ref="cellZone" minOccurs="0" maxOccurs="unbounded"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="boundaryConditions">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element ref="boundaryCondition" minOccurs="0" maxOccurs="unbounded"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="numericalConditions" type="numericalConditionsType"/>

                <xs:element name="monitors">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="forces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="forceMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="points">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="pointMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="surfaces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="surfaceMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="volumes">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="volumeMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>

                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="initialization">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="initialValues">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="velocity" type="vector"/>
                                        <xs:element name="pressure" type="inputNumberType" default="0"/>
                                        <xs:element name="temperature" type="inputNumberType" default="300"/>
                                        <xs:element name="scaleOfVelocity" type="inputNumberType" default="1"/>
                                        <xs:element name="turbulentIntensity" default="1">
                                            <xs:simpleType>
                                                <xs:restriction base="inputNumberType">
                                                    <xs:minInclusive value="0"/>
                                                    <xs:maxInclusive value="100"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="turbulentViscosity" type="inputNumberType" default="10"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <!-- ToDo: Options -->
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="runCalculation">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="runConditions">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="numberOfIterations" type="xs:nonNegativeInteger" default="0"/>
                                        <xs:element name="timeSteppingMethod" default="fixed">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="fixed"/>
                                                    <xs:enumeration value="adaptive"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="timeStepSize" type="inputNumberType" default="0.01"/>
                                        <xs:element name="maxCourantNumber" type="inputNumberType" default="1"/>
                                        <xs:element name="endTime" type="inputNumberType" default="0"/>
                                        <xs:element name="reportIntervalSteps" type="xs:positiveInteger" default="1"/>
                                        <xs:element name="reportIntervalSeconds" type="inputNumberType" default="0.01"/>
                                        <xs:element name="retainOnlyTheMostRecentFiles" type="xs:boolean"
                                                    default="false"/>
                                        <xs:element name="maximumNumberOfDataFiles" type="xs:positiveInteger"
                                                    default="1"/>
                                        <xs:element name="dataWriteFormat" default="binary">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="binary"/>
                                                    <xs:enumeration value="ascii"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="dataWritePrecision" type="xs:positiveInteger" default="6"/>
                                        <xs:element name="timePrecision" type="xs:positiveInteger" default="6"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="parallel">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="numberOfCores" type="xs:positiveInteger"/>
                                        <xs:element name="localhost" type="xs:boolean"/>
                                        <xs:element name="hostfile" type="xs:string"/> <!-- HDF5 Dataset -->
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="version" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
