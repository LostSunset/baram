    <xs:element name="surfaceMonitor">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="reportType" default="areaWeightedAverage">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="areaWeightedAverage"/>
                            <xs:enumeration value="Integral"/>
                            <xs:enumeration value="flowRate"/>
                            <xs:enumeration value="minimum"/>
                            <xs:enumeration value="maximum"/>
                            <xs:enumeration value="cov"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="field" type="scalarFieldType"/>
                <xs:element name="surface" type="xs:nonNegativeInteger"/>  <!-- boundary condition bcid -->
            </xs:sequence>
        </xs:complexType>
