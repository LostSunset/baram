<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.baramcfd.org/baram"
           xmlns="http://www.baramcfd.org/baram" elementFormDefault="qualified" version="1">

    <!-- type definitions -->
    <xs:simpleType name="inputNumberType">
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="inputNumberListType">
        <xs:list itemType="xs:double"/>
    </xs:simpleType>

    <xs:complexType name="vector">
        <xs:sequence>
            <xs:element name="x" type="inputNumberType"/>
            <xs:element name="y" type="inputNumberType"/>
            <xs:element name="z" type="inputNumberType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="temporalScalarListType">
        <xs:sequence>
            <xs:element name="t" type="inputNumberListType"/>
            <xs:element name="v" type="inputNumberListType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="temporalVectorListType">
        <xs:sequence>
            <xs:element name="t" type="inputNumberListType"/>
            <xs:element name="x" type="inputNumberListType"/>
            <xs:element name="y" type="inputNumberListType"/>
            <xs:element name="z" type="inputNumberListType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="scalarFieldType">
        <xs:sequence>
            <xs:element name="field" default="pressure">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="pressure"/>
                        <xs:enumeration value="speed"/>
                        <xs:enumeration value="xVelocity"/>
                        <xs:enumeration value="yVelocity"/>
                        <xs:enumeration value="zVelocity"/>
                        <xs:enumeration value="turbulentKineticEnergy"/>
                        <xs:enumeration value="turbulentDissipationRate"/>
                        <xs:enumeration value="specificDissipationRate"/>
                        <xs:enumeration value="modifiedTurbulentViscosity"/>
                        <xs:enumeration value="temperature"/>
                        <xs:enumeration value="density"/>
                        <xs:enumeration value="modifiedPressure"/>
                        <xs:enumeration value="material"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="mid" type="xs:positiveInteger" default="1"/>  <!-- set if "field" is "material" -->
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="generalType">
        <xs:sequence>
            <xs:element name="flowType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="compressible"/>
                        <xs:enumeration value="incompressible"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="timeTransient" type="xs:boolean"/>
            <xs:element name="solverType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="pressureBased"/>
                        <xs:enumeration value="densityBased"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="operatingConditions">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="gravity">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="direction" type="vector"/>
                                </xs:sequence>
                                <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="pressure" type="inputNumberType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="atmosphericBoundaryLayer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="flowDirection" type="vector"/>
                        <xs:element name="groundNormalDirection" type="vector"/>
                        <xs:element name="referenceFlowSpeed" type="inputNumberType" default="10"/>
                        <xs:element name="referenceHeight" type="inputNumberType" default="10"/>
                        <xs:element name="surfaceRoughnessLength" type="inputNumberType" default="0.01"/>
                        <xs:element name="minimumZCoordinate" type="inputNumberType" default="0.0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="referenceValuesType">
        <xs:sequence>
            <xs:element name="area" type="inputNumberType" default="1"/>
            <xs:element name="density" type="inputNumberType" default="1"/>
            <xs:element name="length" type="inputNumberType" default="1"/>
            <xs:element name="velocity" type="inputNumberType" default="1"/>
            <xs:element name="pressure" type="inputNumberType" default="0"/>
            <xs:element name="referencePressureLocation" type="vector"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="materialType">
        <xs:all>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="chemicalFormula" type="xs:string" minOccurs="0"/>
            <xs:element name="phase">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="gas"/>
                        <xs:enumeration value="liquid"/>
                        <xs:enumeration value="solid"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="density">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="perfectGas"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="specificHeat">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="viscosity" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                    <xs:enumeration value="sutherland"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                        <xs:element name="sutherland" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="coefficient" type="inputNumberType"/>
                                    <xs:element name="temperature" type="inputNumberType"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="thermalConductivity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="molecularWeight" type="inputNumberType" minOccurs="0"/>
            <xs:element name="absorptionCoefficient" type="inputNumberType" minOccurs="0"/>
            <xs:element name="surfaceTension" type="inputNumberType" minOccurs="0"/>
            <xs:element name="saturationPressure" type="inputNumberType" minOccurs="0"/>
            <xs:element name="emissivity" type="inputNumberType" minOccurs="0"/>
        </xs:all>
        <xs:attribute name="mid" type="xs:positiveInteger" use="required"/>
    </xs:complexType>

    <xs:complexType name="multiphaseModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="off"/>
                        <xs:enumeration value="volumeOfFluid"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="turbulenceModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="inviscid"/>
                        <xs:enumeration value="laminar"/>
                        <xs:enumeration value="spalartAllmaras"/>
                        <xs:enumeration value="k-epsilon"/>
                        <xs:enumeration value="k-omega"/>
                        <xs:enumeration value="les"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="k-epsilon">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="standard"/>
                                    <xs:enumeration value="rng"/>
                                    <xs:enumeration value="realizable"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="realizable">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="nearWallTreatment">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="standardWallFunctions"/>
                                                <xs:enumeration value="enhancedWallTreatment"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="k-omega">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="SST"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="energyPrandtlNumber" type="inputNumberType" default="0.85"/>
            <xs:element name="wallPrandtlNumber" type="inputNumberType" default="0.85"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="mrfZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="inputNumberType" default="0"/>
            <xs:element name="rotationAxisOrigin" type="vector"/>
            <xs:element name="rotationAxisDirection" type="vector"/>
            <xs:element name="staticBoundaries">
                <xs:simpleType>
                    <xs:list itemType="xs:positiveInteger"/>  <!-- boundary condition bcid -->
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="porousZoneType">
        <xs:sequence>
            <xs:element name="model" default="darcyForchheimer">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="darcyForchheimer"/>
                        <xs:enumeration value="powerLaw"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="darcyForchheimer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="direction1Vector" type="vector"/>
                        <xs:element name="direction2Vector" type="vector"/>
                        <xs:element name="viscousResistanceCoefficient" type="vector"/>
                        <xs:element name="inertialResistanceCoefficient" type="vector"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="powerLaw">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="c0" type="inputNumberType" default="0"/>
                        <xs:element name="c1" type="inputNumberType" default="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="slidingMeshZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="inputNumberType" default="1000"/>
            <xs:element name="rotationAxisOrigin" type="vector"/>
            <xs:element name="rotationAxisDirection" type="vector"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="actuatorDiskZoneType">
        <xs:sequence>
            <xs:element name="diskDirection" type="vector"/>
            <xs:element name="powerCoefficient" type="inputNumberType" default="0"/>
            <xs:element name="thrustCoefficient" type="inputNumberType" default="0"/>
            <xs:element name="diskArea" type="inputNumberType" default="0"/>
            <xs:element name="forceComputation" default="Froude">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Froude"/>
                        <xs:enumeration value="variableScaling"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="cellZone">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="zoneType" default="none">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="none"/>
                            <xs:enumeration value="mrf"/>
                            <xs:enumeration value="porous"/>
                            <xs:enumeration value="actuatorDisk"/>
                            <xs:enumeration value="slidingMesh"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="mrf" type="mrfZoneType"/>
                <xs:element name="porous" type="porousZoneType"/>
                <xs:element name="slidingMesh" type="slidingMeshZoneType"/>
                <xs:element name="actuatorDisk" type="actuatorDiskZoneType"/>
                <xs:element name="sourceTerms">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="mass">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="specification" default="constant">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="constant"/>
                                                    <xs:enumeration
                                                            value="piecewiseLinear"/>
                                                    <xs:enumeration
                                                            value="polynomial"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                        <xs:element
                                                name="piecewiseLinear" type="temporalScalarListType"/>
                                        <xs:element name="polynomial" type="inputNumberListType"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="energy">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="specification" default="constant">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="constant"/>
                                                    <xs:enumeration
                                                            value="piecewiseLinear"/>
                                                    <xs:enumeration
                                                            value="polynomial"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                        <xs:element
                                                name="piecewiseLinear" type="temporalScalarListType"/>
                                        <xs:element name="polynomial" type="inputNumberListType"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="turbulentKineticEnergy">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="turbulentDissipationRate">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="specificDissipationRate">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="modifiedTurbulentViscosity">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="unit"
                                                    default="valueForEntireCellZone">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration
                                                            value="valuePerUnitVolume"/>
                                                    <xs:enumeration
                                                            value="valueForEntireCellZone"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="constant"
                                                    type="inputNumberType" default="0"/>
                                    </xs:sequence>
                                    <xs:attribute name="disabled" type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="fixedValues">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="velocity">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="velocity" type="vector"/>
                                        <xs:element name="relaxation" default="1">
                                            <xs:simpleType>
                                                <xs:restriction base="inputNumberType">
                                                    <xs:minInclusive value="0"/>
                                                    <xs:maxInclusive value="1"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="disabled"
                                                  type="xs:boolean"
                                                  use="required"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="temperature" default="300">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="inputNumberType">
                                            <xs:attribute name="disabled"
                                                          type="xs:boolean"
                                                          use="required"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="turbulentKineticEnergy" default="0">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="inputNumberType">
                                            <xs:attribute name="disabled"
                                                          type="xs:boolean"
                                                          default="true"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="turbulentDissipationRate" default="0">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="inputNumberType">
                                            <xs:attribute name="disabled"
                                                          type="xs:boolean"
                                                          default="true"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="specificDissipationRate" default="0">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="inputNumberType">
                                            <xs:attribute name="disabled"
                                                          type="xs:boolean"
                                                          default="true"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="modifiedTurbulentViscosity" default="0">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="inputNumberType">
                                            <xs:attribute name="disabled"
                                                          type="xs:boolean"
                                                          default="true"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="czid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="turbulenceBoundaryConditionType">
        <xs:sequence>
            <xs:element name="k-epsilon">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="intensityAndViscosityRatio">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="kAndEpsilon"/>
                                    <xs:enumeration value="intensityAndViscosityRatio"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="turbulentKineticEnergy" type="inputNumberType" default="1"/>
                        <xs:element name="turbulentDissipationRate" type="inputNumberType" default="1"/>
                        <xs:element name="turbulentIntensity" type="inputNumberType" default="1"/>
                        <xs:element name="turbulentViscosityRatio" type="inputNumberType" default="10"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="k-omega">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="intensityAndViscosityRatio">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="kAndOmega"/>
                                    <xs:enumeration value="intensityAndViscosityRatio"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="turbulentKineticEnergy" type="inputNumberType" default="1"/>
                        <xs:element name="specificDissipationRate" type="inputNumberType" default="1"/>
                        <xs:element name="turbulentIntensity" type="inputNumberType" default="1"/>
                        <xs:element name="turbulentViscosityRatio" type="inputNumberType" default="10"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="spalartAllmaras">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="modifiedTurbulentViscosity">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="modifiedTurbulentViscosity"/>
                                    <xs:enumeration value="turbulentViscosityRatio"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="modifiedTurbulentViscosity" type="inputNumberType" default="0"/>
                        <xs:element name="turbulentViscosityRatio" type="inputNumberType" default="10"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="temperatureBoundaryConditionType">
        <xs:sequence>
            <xs:element name="profile" default="constant">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="constant"/>
                        <xs:enumeration value="spatialDistribution"/>
                        <xs:enumeration value="temporalDistribution"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="constant" type="inputNumberType" default="300"/>
            <xs:element name="spatialDistribution" type="xs:string"/> <!-- HDF5 Dataset -->
            <xs:element name="temporalDistribution">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="piecewiseLinear"/>
                                    <xs:enumeration value="polynomial"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="piecewiseLinear" type="temporalScalarListType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="velocityInletType">
        <xs:sequence>
            <xs:element name="velocity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="magnitudeNormal">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="component"/>
                                    <xs:enumeration value="magnitudeNormal"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="component">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="profile" default="constant">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="constant"/>
                                                <xs:enumeration value="spatialDistribution"/>
                                                <xs:enumeration value="temporalDistribution"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="constant" type="vector"/>
                                    <xs:element name="spatialDistribution" type="xs:string"/> <!-- HDF5 Dataset -->
                                    <xs:element name="temporalDistribution">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="specification" default="piecewiseLinear">
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:enumeration value="piecewiseLinear"/>
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="piecewiseLinear" type="temporalVectorListType"/>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="magnitudeNormal">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="profile" default="constant">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="constant"/>
                                                <xs:enumeration value="spatialDistribution"/>
                                                <xs:enumeration value="temporalDistribution"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="constant" type="inputNumberType" default="0"/>
                                    <xs:element name="spatialDistribution" type="xs:string"/> <!-- HDF5 Dataset -->
                                    <xs:element name="temporalDistribution">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="specification" default="piecewiseLinear">
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:enumeration value="piecewiseLinear"/>
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="piecewiseLinear" type="temporalScalarListType"/>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="flowRateInletType">
        <xs:sequence>
            <xs:element name="flowRate">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="massFlowRate">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="volumeFlowRate"/>
                                    <xs:enumeration value="massFlowRate"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="volumeFlowRate" type="inputNumberType" default="0"/>
                        <xs:element name="massFlowRate" type="inputNumberType" default="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="pressureInletType">
        <xs:sequence>
            <xs:element name="pressure" type="inputNumberType" default="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="pressureOutletType">
        <xs:sequence>
            <xs:element name="totalPressure" type="inputNumberType" default="0"/>
            <xs:element name="calculatedBackflow" type="xs:boolean" default="false"/>
            <xs:element name="backflowTotalTemperature" type="inputNumberType" default="300"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="openChannelInletType">
        <xs:sequence>
            <xs:element name="volumeFlowRate" type="inputNumberType" default="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="openChannelOutletType">
        <xs:sequence>
            <xs:element name="meanVelocity" type="inputNumberType" default="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="freeStreamType">
        <xs:sequence>
            <xs:element name="streamVelocity" type="vector"/>
            <xs:element name="pressure" type="inputNumberType" default="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="farFieldRiemannType">
        <xs:sequence>
            <xs:element name="flowDirection" type="vector"/>
            <xs:element name="machNumber" type="inputNumberType" default="1"/>
            <xs:element name="staticPressure" type="inputNumberType" default="101325"/>
            <xs:element name="staticTemperature" type="inputNumberType" default="300"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="subsonicInflowType">
        <xs:sequence>
            <xs:element name="flowDirection" type="vector"/>
            <xs:element name="totalPressure" type="inputNumberType" default="101325"/>
            <xs:element name="totalTemperature" type="inputNumberType" default="300"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="subsonicOutflowType">
        <xs:sequence>
            <xs:element name="staticPressure" type="inputNumberType" default="101325"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="supersonicInflowType">
        <xs:sequence>
            <xs:element name="velocity" type="vector"/>
            <xs:element name="staticPressure" type="inputNumberType" default="101325"/>
            <xs:element name="staticTemperature" type="inputNumberType" default="300"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="wallType">
        <xs:sequence>
            <xs:element name="velocity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="type" default="noSlip">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="noSlip"/>
                                    <xs:enumeration value="slip"/>
                                    <xs:enumeration value="movingWall"/>
                                    <xs:enumeration value="atmosphericWall"/>
                                    <xs:enumeration value="translationalMovingWall"/>
                                    <xs:enumeration value="rotationalMovingWall"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="translationalMovingWall">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="velocity" type="vector"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="rotationalMovingWall">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="speed" type="inputNumberType" default="100"/>
                                    <xs:element name="rotationAxisOrigin" type="vector"/>
                                    <xs:element name="rotationAxisDirection" type="vector"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="temperature">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="type" default="adiabatic">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="adiabatic"/>
                                    <xs:enumeration value="constantTemperature"/>
                                    <xs:enumeration value="constantHeatFlux"/>
                                    <xs:enumeration value="convection"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="temperature" type="inputNumberType" default="300"/>
                        <xs:element name="heatFlux" type="inputNumberType" default="1000"/>
                        <xs:element name="heatTransferCoefficient" type="inputNumberType" default="100"/>
                        <xs:element name="freeStreamTemperature" type="inputNumberType" default="300"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="radiation">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="wallEmissivity" default="1">
                            <xs:simpleType>
                                <xs:restriction base="inputNumberType">
                                    <xs:minInclusive value="0"/>
                                    <xs:maxInclusive value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="radiativeFluxRelaxation" default="1">
                            <xs:simpleType>
                                <xs:restriction base="inputNumberType">
                                    <xs:minInclusive value="0"/>
                                    <xs:maxInclusive value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="interfaceType">
        <xs:sequence>
            <xs:element name="mode" default="internalInterface">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="internalInterface"/>
                        <xs:enumeration value="rotationalPeriodic"/>
                        <xs:enumeration value="translationalPeriodic"/>
                        <xs:enumeration value="regionInterface"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="rotationAxisOrigin" type="vector"/>
            <xs:element name="rotationAxisDirection" type="vector"/>
            <xs:element name="translationVector" type="vector"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="porousJumpType">
        <xs:sequence>
            <xs:element name="darcyCoefficient" type="inputNumberType" default="100"/>
            <xs:element name="inertialCoefficient" type="inputNumberType" default="5"/>
            <xs:element name="porousMediaThickness" type="inputNumberType" default="0.05"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="fanType">
        <xs:sequence>
            <xs:element name="fanCurveFile" type="xs:string"/> <!-- HDF5 Dataset -->
            <xs:element name="reverseDirection" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="boundaryCondition">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="geometricalType" default="wall">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="group"/>
                            <xs:enumeration value="patch"/>
                            <xs:enumeration value="symmetry"/>
                            <xs:enumeration value="symmetryPlane"/>
                            <xs:enumeration value="empty"/>
                            <xs:enumeration value="wedge"/>
                            <xs:enumeration value="cyclic"/>
                            <xs:enumeration value="cyclicAMI"/>
                            <xs:enumeration value="wall"/>
                            <xs:enumeration value="processor"/>
                            <xs:enumeration value="mappedWall"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="physicalType" default="wall">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="velocityInlet"/>
                            <xs:enumeration value="flowRateInlet"/>
                            <xs:enumeration value="pressureInlet"/>
                            <xs:enumeration value="pressureOutlet"/>
                            <xs:enumeration value="ablInlet"/>
                            <xs:enumeration value="openChannelInlet"/>
                            <xs:enumeration value="openChannelOutlet"/>
                            <xs:enumeration value="outflow"/>
                            <xs:enumeration value="freeStream"/>
                            <xs:enumeration value="farFieldRiemann"/>
                            <xs:enumeration value="subsonicInflow"/>
                            <xs:enumeration value="subsonicOutflow"/>
                            <xs:enumeration value="supersonicInflow"/>
                            <xs:enumeration value="supersonicOutflow"/>
                            <xs:enumeration value="wall"/>
                            <xs:enumeration value="thermoCoupledWall"/>
                            <xs:enumeration value="symmetry"/>
                            <xs:enumeration value="interface"/>
                            <xs:enumeration value="porousJump"/>
                            <xs:enumeration value="fan"/>
                            <xs:enumeration value="empty"/>
                            <xs:enumeration value="cyclic"/>
                            <xs:enumeration value="wedge"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="group" type="xs:nonNegativeInteger" default="0"/> <!-- bcid of boundary group -->
                <xs:element name="members"> <!-- bcid of group members when geometricalType is "group" -->
                    <xs:simpleType>
                        <xs:list itemType="xs:positiveInteger"/>
                    </xs:simpleType>
                </xs:element>

                <xs:element name="velocityInlet" type="velocityInletType"/>
                <xs:element name="flowRateInlet" type="flowRateInletType"/>
                <xs:element name="pressureInlet" type="pressureInletType"/>
                <xs:element name="pressureOutlet" type="pressureOutletType"/>
                <xs:element name="openChannelInlet" type="openChannelInletType"/>
                <xs:element name="openChannelOutlet" type="openChannelOutletType"/>
                <!-- no setting for "outflow" -->
                <!-- <xs:element name="outflow" type="outflowType" /> -->
                <xs:element name="freeStream" type="freeStreamType"/>
                <xs:element name="farFieldRiemann" type="farFieldRiemannType"/>
                <xs:element name="subsonicInflow" type="subsonicInflowType"/>
                <xs:element name="subsonicOutflow" type="subsonicOutflowType"/>
                <xs:element name="supersonicInflow" type="supersonicInflowType"/>
                <!-- no setting for supersonicOutflow -->
                <!-- <xs:element name="supersonicOutflow" type="supersonicOutflowType" /> -->
                <xs:element name="wall" type="wallType"/>
                <!-- no setting for thermoCoupledWall -->
                <!-- <xs:element name="thermoCoupledWall" type="thermoCoupledWallType" /> -->
                <!-- no setting for symmetry -->
                <!--  <xs:element name="symmetry" type="symmetryType" /> -->
                <xs:element name="interface" type="interfaceType"/>
                <xs:element name="porousJump" type="porousJumpType"/>
                <xs:element name="fan" type="fanType"/>
                <!-- no setting for empty -->
                <!-- <xs:element name="empty" type="emptyType" /> -->
                <!-- xs:element name="cyclic" type="cyclicType"/> -->
                <!-- no setting for wedge -->
                <!-- <xs:element name="wedge" type="wedgeType" /> -->

                <xs:element name="turbulence" type="turbulenceBoundaryConditionType"/>
                <xs:element name="temperature" type="temperatureBoundaryConditionType"/>
                <xs:element name="coupledBoundary" type="xs:nonNegativeInteger" default="0"/>
            </xs:sequence>
            <xs:attribute name="bcid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="numericalConditionsType">
        <xs:sequence>
            <xs:element name="pressureVelocityCouplingScheme">
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='SIMPLE'/>
                        <xs:enumeration value='SIMPLEC'/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="useMomentumPredictor" type="xs:boolean" default="true"/>
            <xs:element name="discretizationSchemes">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="time" default="firstOrderImplicit">
                            <xs:simpleType>
                                <xs:restriction base='xs:string'>
                                    <xs:enumeration value='firstOrderImplicit'/>
                                    <xs:enumeration value='secondOrderImplicit'/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="momentum" default="secondOrderUpwind">
                            <xs:simpleType>
                                <xs:restriction base='xs:string'>
                                    <xs:enumeration value='firstOrderUpwind'/>
                                    <xs:enumeration value='secondOrderUpwind'/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="energy" default="secondOrderUpwind">
                            <xs:simpleType>
                                <xs:restriction base='xs:string'>
                                    <xs:enumeration value='firstOrderUpwind'/>
                                    <xs:enumeration value='secondOrderUpwind'/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="turbulentKineticEnergy" default="firstOrderUpwind">
                            <xs:simpleType>
                                <xs:restriction base='xs:string'>
                                    <xs:enumeration value='firstOrderUpwind'/>
                                    <xs:enumeration value='secondOrderUpwind'/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="volumeFraction" default="firstOrderUpwind">
                            <xs:simpleType>
                                <xs:restriction base='xs:string'>
                                    <xs:enumeration value='firstOrderUpwind'/>
                                    <xs:enumeration value='secondOrder'/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="underRelaxationFactors">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pressure" type="inputNumberType" default="0.3"/>
                        <xs:element name="pressureFinal" type="inputNumberType" default="1"/>
                        <xs:element name="density" type="inputNumberType" default="0.9"/>
                        <xs:element name="densityFinal" type="inputNumberType" default="1"/>
                        <xs:element name="momentum" type="inputNumberType" default="0.7"/>
                        <xs:element name="momentumFinal" type="inputNumberType" default="1"/>
                        <xs:element name="energy" type="inputNumberType" default="0.9"/>
                        <xs:element name="energyFinal" type="inputNumberType" default="1"/>
                        <xs:element name="turbulence" type="inputNumberType" default="0.7"/>
                        <xs:element name="turbulenceFinal" type="inputNumberType" default="1"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="highOrderTermRelaxation">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="relaxationFactor" default="0.5">
                            <xs:simpleType>
                                <xs:restriction base="inputNumberType">
                                    <xs:minInclusive value="0"/>
                                    <xs:maxInclusive value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="maxIterationsPerTimeStep" type="xs:positiveInteger" default="20"/>
            <xs:element name="numberOfCorrectors" type="xs:positiveInteger" default="4"/>
            <xs:element name="convergenceCriteria">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pressure">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="inputNumberType" default="0.001"/>
                                    <xs:element name="relative" type="inputNumberType" default="0.5"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="momentum">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="inputNumberType" default="0.001"/>
                                    <xs:element name="relative" type="inputNumberType" default="0.5"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="energy">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="inputNumberType" default="0.001"/>
                                    <xs:element name="relative" type="inputNumberType" default="0.5"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="turbulence">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="inputNumberType" default="0.001"/>
                                    <xs:element name="relative" type="inputNumberType" default="0.5"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="advanced">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="limits">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="minimumStaticTemperature" type="inputNumberType" default="1"/>
                                    <xs:element name="maximumStaticTemperature" type="inputNumberType" default="5000"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="forceMonitor">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="dragDirection" type="vector"/>
                <xs:element name="liftDirection" type="vector"/>
                <xs:element name="pitchAxisDirection" type="vector"/>
                <xs:element name="centerOfRotation" type="vector"/>
                <xs:element name="region" type="xs:string"/>
                <xs:element name="boundaries">
