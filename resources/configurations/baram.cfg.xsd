    <xs:element name="pointMonitor">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="field" type="scalarFieldType"/>
                <xs:element name="interval" type="xs:positiveInteger" default="1"/>
                <xs:element name="coordinate" type="vector"/>
                <xs:element name="snapOntoBoundary" type="xs:boolean" default="false"/>
