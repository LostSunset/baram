<?xml version="1.0" encoding="UTF-8"?>
<configuration version="1"  xmlns="http://www.example.org/baram">
    <general>
        <flowType>compressible</flowType>
        <timeTransient>false</timeTransient>
        <solverType>pressureBased</solverType>
        <operatingConditions>
            <gravity>
                <direction>
                    <x>0</x>
                    <y>0</y>
                    <z>0</z>
                </direction>
            </gravity>
            <pressure>101325</pressure>
            <referencePressureLocation>
                <x>0</x>
                <y>0</y>
                <z>0</z>
            </referencePressureLocation>
        </operatingConditions>
    </general>
    
    <materials>
        <material id="1">
            <name>air</name>
            <phase>gas</phase>
            <density>
                <specification>constant</specification>
                <value>1.225</value>
            </density>
            <specificHeat>
                <specification>constant</specification>
                <value>1006</value>
                <polynomial>0</polynomial>
            </specificHeat>
            <viscosity>
                <specification>constant</specification>
                <value>0.0000179</value>
                <polynomial>0</polynomial>
                <sutherland>
                    <coefficient>0.00000146</coefficient>
                    <temperature>110.4</temperature>
                </sutherland>
            </viscosity>
            <thermalConductivity>
                <specification>constant</specification>
                <value>0.0245</value>
                <polynomial>0</polynomial>
            </thermalConductivity>
            <molecularWeight>28.966</molecularWeight>
            <absorptionCoefficient>0</absorptionCoefficient>
            <surfaceTension>0</surfaceTension>
            <saturationPressure>0</saturationPressure>
            <emissivity>0</emissivity>
        </material>
        <material id="2">
            <name>aluminum</name>
            <phase>solid</phase>
            <density>
                <specification>constant</specification>
                <value>2707</value>
            </density>
            <specificHeat>
                <specification>constant</specification>
                <value>896</value>
                <polynomial>0</polynomial>
            </specificHeat>
            <viscosity>
                <specification>constant</specification>
                <value>0</value>
                <polynomial>0</polynomial>
                <sutherland>
                    <coefficient>0</coefficient>
                    <temperature>0</temperature>
                </sutherland>
            </viscosity>
            <thermalConductivity>
                <specification>constant</specification>
                <value>204</value>
                <polynomial>0</polynomial>
            </thermalConductivity>
            <molecularWeight>0</molecularWeight>
            <absorptionCoefficient>0</absorptionCoefficient>
            <surfaceTension>0</surfaceTension>
            <saturationPressure>0</saturationPressure>
            <emissivity>0.039</emissivity>
        </material>
    </materials>
    
    <models>
        <multiphaseModels>
            <model>off</model>
        </multiphaseModels>
        <energyModels>off</energyModels>
        <turbulenceModels>
            <model>laminar</model>
            <k-epsilon>
                <model>standard</model>
                <realizable>
                    <nearWallTreatment>standardWallFunctions</nearWallTreatment>
                </realizable>
            </k-epsilon>
            <k-omega>
                <model>SST</model>
            </k-omega>
        </turbulenceModels>
        <speciesModels>off</speciesModels>
    </models>
    
    <cellZones>
    </cellZones>
    
    <boundaryConditions>
    </boundaryConditions>
    
    <numericalConditions>
        <pressureVelocityCouplingScheme>SIMPLE</pressureVelocityCouplingScheme>
        <discretizationSchemes>
            <time>firstOrderImplicit</time>
            <momentum>secondOrderUpwind</momentum>
            <energy>secondOrderUpwind</energy>
            <turbulentKineticEnergy>firstOrderUpwind</turbulentKineticEnergy>
        </discretizationSchemes>
        <underRelaxationFactors>
            <pressure>0.3</pressure>
            <pressureFinal>1</pressureFinal>
            <momentum>0.7</momentum>
            <momentumFinal>1</momentumFinal>
            <energy>0.9</energy>
            <energyFinal>1</energyFinal>
            <turbulence>0.7</turbulence>
            <turbulenceFinal>1</turbulenceFinal>
        </underRelaxationFactors>
        <maxIterationsPerTimeStep>20</maxIterationsPerTimeStep>
        <numberOfCorrectors>4</numberOfCorrectors>
        <convergenceCriteria>
            <pressure>
                <absolute>0.001</absolute>
                <relative>0.5</relative>
            </pressure>
            <momentum>
                <absolute>0.001</absolute>
                <relative>0.5</relative>
            </momentum>
            <energy>
                <absolute>0.001</absolute>
                <relative>0.5</relative>
            </energy>
            <turbulence>
                <absolute>0.001</absolute>
                <relative>0.5</relative>
            </turbulence>
        </convergenceCriteria>
        <advanced>
            <limits>
                <minimumStaticTemperature>1</minimumStaticTemperature>
                <maximumStaticTemperature>5000</maximumStaticTemperature>
            </limits>
        </advanced>
    </numericalConditions>
    
    <monitors>
        <forces></forces>
        <points></points>
        <surfaces></surfaces>
        <volumes></volumes>
    </monitors>
    
    <initialization>
        <initialValues>
            <velocity>
                <x>0</x>
                <y>0</y>
                <z>0</z>
            </velocity>
            <pressure>0</pressure>
            <temperature>300</temperature>
            <scaleOfVelocity>0</scaleOfVelocity>
            <turbulentIntensity>10</turbulentIntensity>
            <turbulentViscosity>10</turbulentViscosity>
        </initialValues>
    </initialization>

    <runCalculation>
        <runConditions>
            <numberOfIterations>0</numberOfIterations>
            <timeSteppingMethod>fixed</timeSteppingMethod>
            <timeStepSize>0.01</timeStepSize>
            <endTime>0</endTime>
            <reportIntervalSteps>1</reportIntervalSteps>
            <reportIntervalSeconds>0.01</reportIntervalSeconds>
            <retainOnlyTheMostRecentFiles>false</retainOnlyTheMostRecentFiles>
            <maximumNumberOfDataFiles>1</maximumNumberOfDataFiles>
            <dataWriteFormat>binary</dataWriteFormat>
            <dataWritePrecision>6</dataWritePrecision>
            <timePrecision>6</timePrecision>
        </runConditions>
        <parallel>
            <numberOfCores>1</numberOfCores>
            <machineType>smp</machineType>
        </parallel>
    </runCalculation>
</configuration>