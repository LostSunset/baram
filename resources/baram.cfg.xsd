                                        </xs:element>

                                        <xs:element name="timeStepSize" type="xs:decimal" default="1" />

                                        <xs:element name="endTime" type="xs:decimal" />

                                        <xs:element name="reportIntervalSteps" type="xs:positiveInteger" default="1"/>

                                        <xs:element name="reportIntervalSeconds" type="xs:decimal" default="1" />

                                        <xs:element name="retainOnlyTheMostRecentFiles" type="xs:boolean" default="false"/>

                                        <xs:element name="maximumNumberOfDataFiles" type="xs:positiveInteger" default="1" />

                                        <xs:element name="dataWriteFormat" default="ascii">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
