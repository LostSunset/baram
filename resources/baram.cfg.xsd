            <xs:element name="freeStream" type="freeStreamType" />

            <xs:element name="farFieldRiemann" type="farFieldRiemannType" />

            <xs:element name="subsonicInflow" type="subsonicInflowType" />

            <xs:element name="subsonicOutflow" type="subsonicOutflowType" />

            <xs:element name="supersonicInflow" type="supersonicInflowType" />

            <!-- no setting for supersonicOutflow -->
            <!-- <xs:element name="supersonicOutflow" type="supersonicOutflowType" /> -->

            <xs:element name="wall" type="wallType" />

            <!-- no setting for thermoCoupledWall -->
            <!-- <xs:element name="thermoCoupledWall" type="thermoCoupledWallType" /> -->

            <!-- no setting for symmetry -->
            <!--  <xs:element name="symmetry" type="symmetryType" /> -->

            <xs:element name="interface" type="interfaceType" />

            <xs:element name="porousJump" type="porousJumpType" />

            <xs:element name="fan" type="fanType" />

            <!-- no setting for empty -->
            <!-- <xs:element name="empty" type="emptyType" /> -->

            <xs:element name="cyclic" type="cyclicType" />

            <!-- no setting for wedge -->
            <!-- <xs:element name="wedge" type="wedgeType" /> -->
        
        </xs:sequence>
    </xs:complexType>



    <xs:complexType name="numericalConditionsType">
        <xs:sequence>
            <xs:element name="pressureVelocityCouplingScheme">
                <xs:simpleType>
                    <xs:restriction base='xs:string'>
                        <xs:enumeration value='SIMPLE' />
                        <xs:enumeration value='SIMPLEC' />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>


            <xs:element name="discretizationSchemes">
                <xs:complexType>
                    <xs:sequence>
                <xs:element name="time" default="firstOrderImplicit">
                    <xs:simpleType>
                        <xs:restriction base='xs:string'>
                            <xs:enumeration value='firstOrderImplicit' />
                            <xs:enumeration value='secondOrderImplicit' />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="momentum" default="secondOrderUpwind">
                    <xs:simpleType>
                        <xs:restriction base='xs:string'>
                            <xs:enumeration value='firstOrderUpwind' />
                            <xs:enumeration value='secondOrderUpwind' />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="energy" default="secondOrderUpwind">
                    <xs:simpleType>
                        <xs:restriction base='xs:string'>
                            <xs:enumeration value='firstOrderUpwind' />
                            <xs:enumeration value='secondOrderUpwind' />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="turbulentKineticEnergy" default="firstOrderUpwind">
                    <xs:simpleType>
                        <xs:restriction base='xs:string'>
                            <xs:enumeration value='firstOrderUpwind' />
                            <xs:enumeration value='secondOrderUpwind' />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="underRelaxationFactors">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pressure" type="xs:decimal" default="0.3"/>
                        <xs:element name="pressureFinal" type="xs:decimal" default="1"/>
                        <xs:element name="momentum" type="xs:decimal" default="0.7"/>
                        <xs:element name="momentumFinal" type="xs:decimal" default="1"/>
                        <xs:element name="energy" type="xs:decimal" default="0.9"/>
                        <xs:element name="energyFinal" type="xs:decimal" default="1"/>
                        <xs:element name="turbulence" type="xs:decimal" default="0.7"/>
                        <xs:element name="turbulenceFinal" type="xs:decimal" default="1"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="maxIterationsPerTimeStep" type="xs:positiveInteger" />
            <xs:element name="numberOfCorrectors" type="xs:positiveInteger" default="4" />

            <xs:element name="convergenceCriteria">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pressure">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="xs:decimal" />
                                    <xs:element name="relative" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="momentum">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="xs:decimal" />
                                    <xs:element name="relative" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="energy">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="xs:decimal" />
                                    <xs:element name="relative" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="turbulence">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="absolute" type="xs:decimal" />
                                    <xs:element name="relative" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="advanced">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="subIterationConditions">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="tolerance" type="xs:decimal" />
                                    <xs:element name="relativeTolerance" type="xs:decimal" />
                                    <xs:element name="minimumIteration" type="xs:positiveInteger" />
                                    <xs:element name="maximumIteration" type="xs:positiveInteger" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="limits">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="minimumStaticTemperature" type="xs:decimal" />
                                    <xs:element name="maximumStaticTemperature" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="forceMonitorType">
        <xs:sequence>
