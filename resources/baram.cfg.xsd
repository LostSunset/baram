                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="runCalculation">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="runConditions">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="numberOfIterations" type="xs:nonNegativeInteger" />
                                        <xs:element name="timeSteppingMethod" default="fixed">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="fixed" />
                                                    <xs:enumeration value="adaptive" />
                                                </xs:restriction>
