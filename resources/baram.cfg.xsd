<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/baram"
    xmlns="http://www.example.org/baram" elementFormDefault="qualified">

    <!-- type definitions -->

    <xs:complexType name="vector">
        <xs:sequence>
            <xs:element name="x" type="xs:decimal" />
            <xs:element name="y" type="xs:decimal" />
            <xs:element name="z" type="xs:decimal" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="temporalVector">
        <xs:sequence>
            <xs:element name="t" type="xs:decimal" />
            <xs:element name="x" type="xs:decimal" />
            <xs:element name="y" type="xs:decimal" />
            <xs:element name="z" type="xs:decimal" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="generalType">
        <xs:sequence>
            <xs:element name="flowType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="compressible" />
                        <xs:enumeration value="incompressible" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="timeTransient" type="xs:boolean" />
            <xs:element name="solverDensity" type="xs:boolean" />
            <xs:element name="operatingConditions">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="gravity">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="direction" type="vector" />
                                </xs:sequence>
                                <xs:attribute name="disabled" type="xs:boolean" default="true" />
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="pressure" type="xs:decimal" />
                        <xs:element name="referencePressureLocation" type="vector" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="materialType">
        <xs:sequence>
            <xs:element name="name" type="xs:string" />
            <xs:element name="phase">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="gas" />
                        <xs:enumeration value="liquid" />
                        <xs:enumeration value="solid" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="density">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant" />
                                    <xs:enumeration value="perfectGas" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>

                        <xs:element name="value" type="xs:decimal" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="specificHeat">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant" />
                                    <xs:enumeration value="polynomial" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>

                        <xs:element name="value" type="xs:decimal" />

                        <xs:element name="polynomial">
                            <xs:simpleType>
                                <xs:list itemType="xs:decimal" />
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>



            <xs:element name="viscosity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant" />
                                    <xs:enumeration value="polynomial" />
                                    <xs:enumeration value="sutherland" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>

                        <xs:element name="value" type="xs:decimal" />

                        <xs:element name="polynomial">
                            <xs:simpleType>
                                <xs:list itemType="xs:decimal" />
                            </xs:simpleType>
                        </xs:element>

                        <xs:element name="sutherland">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="coefficient" type="xs:decimal" />
                                    <xs:element name="temperature" type="xs:decimal" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="thermalConductivity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant" />
                                    <xs:enumeration value="polynomial" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>

                        <xs:element name="value" type="xs:decimal" />

                        <xs:element name="polynomial">
                            <xs:simpleType>
                                <xs:list itemType="xs:decimal" />
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="molecularWeight" type="xs:decimal" />
            <xs:element name="absorptionCoefficient" type="xs:decimal" />
            <xs:element name="surfaceTension" type="xs:decimal" />
            <xs:element name="saturationPressure" type="xs:decimal" />
            <xs:element name="emissivity" type="xs:decimal" />
        </xs:sequence>
        <xs:attribute name="id" type="xs:positiveInteger" use="required" />
    </xs:complexType>


    <xs:complexType name="multiphaseModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="off" />
                        <xs:enumeration value="volumeOfFluid" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="turbulenceModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="laminar" />
                        <xs:enumeration value="spalartAllmaras" />
                        <xs:enumeration value="k-epsilon" />
                        <xs:enumeration value="k-omega" />
                        <xs:enumeration value="les" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="k-epsilon">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="standard" />
                                    <xs:enumeration value="rng" />
                                    <xs:enumeration value="realizable" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="realizable">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="nearWallTreatment">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="standardWallFunctions" />
                                                <xs:enumeration value="enhancedWallTreatment" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="k-omega">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="SST" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="mrfZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="xs:decimal" />
            <xs:element name="rotationAxisOrigin" type="vector" />
            <xs:element name="rotationAxisDirection" type="vector" />
            <xs:element name="staticBoundaries">
                <xs:simpleType>
                    <xs:list itemType="xs:positiveInteger" />
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="porousZoneType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="darcyForchheimer" />
                        <xs:enumeration value="powerLaw" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="darcyForchheimer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="direction1Vector" type="vector" />
                        <xs:element name="direction2Vector" type="vector" />
                        <xs:element name="viscousResistanceCoefficient" type="vector" />
                        <xs:element name="inertialResistanceCoefficient" type="vector" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="powerLaw">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="c0" type="xs:decimal" />
                        <xs:element name="c1" type="xs:decimal" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="slidingMeshZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="xs:decimal" />
            <xs:element name="rotationAxisOrigin" type="vector" />
            <xs:element name="rotationAxisDirection" type="vector" />
        </xs:sequence>
    </xs:complexType>
    

    <xs:complexType name="actuatorDiskZoneType">
        <xs:sequence>
            <xs:element name="diskDirection" type="vector" />
            <xs:element name="powerCoefficient" type="xs:decimal" />
            <xs:element name="thrustCoefficient" type="xs:decimal" />
            <xs:element name="diskArea" type="xs:decimal" />
            <xs:element name="upstreamPoint" type="vector" />
        </xs:sequence>
    </xs:complexType>
    
        
    <xs:complexType name="turbulenceBoundaryConditionType">
        <xs:sequence>
            <xs:element name="k-epsilon">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="kAndEpsilon" />
                                    <xs:enumeration value="intensityAndViscosityRatio" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="turbulentKineticEnergy" type="xs:decimal" />
                        <xs:element name="turbulentDissipationRate" type="xs:decimal" />
                        <xs:element name="turbulentIntensity" type="xs:decimal" />
                        <xs:element name="turbulentViscosityRatio" type="xs:decimal" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="k-omega">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="kAndOmega" />
                                    <xs:enumeration value="intensityAndViscosityRatio" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="turbulentKineticEnergy" type="xs:decimal" />
                        <xs:element name="specificDissipationRate" type="xs:decimal" />
                        <xs:element name="turbulentIntensity" type="xs:decimal" />
                        <xs:element name="turbulentViscosityRatio" type="xs:decimal" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="spalartAllmaras">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="modifiedTurbulentViscosity" />
                                    <xs:enumeration value="turbulentViscosityRatio" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="modifiedTurbulentViscosity" type="xs:decimal" />
                        <xs:element name="turbulentViscosityRatio" type="xs:decimal" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="temperatureBoundaryConditionType">
        <xs:sequence>
            <xs:element name="profile">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="constant" />
                        <xs:enumeration value="spatialDistribution" />
                        <xs:enumeration value="temporalDistribution" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="temperature" type="xs:decimal" />
            <xs:element name="spacialDistribution" type="xs:string" /> <!-- HDF5 Dataset -->
            <xs:element name="temporalDistribution">
                <xs:complexType>
                    <xs:sequence>
                <xs:element name="profile">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="directInputPiecewiseLinear" />
                            <xs:enumeration value="directInputPolynomial" />
                            <xs:enumeration value="file" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="directInputPiecewiseLinear">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="piece" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="t" type="xs:decimal" />
                                        <xs:element name="temperature" type="xs:decimal" default="288.15" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="directInputPolynomial">
                    <xs:simpleType>
                        <xs:list itemType="xs:decimal" />
                    </xs:simpleType>
                </xs:element>
                <xs:element name="file" type="xs:string" /> <!-- HDF5 Dataset -->
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

        </xs:sequence>
    </xs:complexType>



    <xs:complexType name="velocityInletType">
        <xs:sequence>
            <xs:element name="velocity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="component" />
                                    <xs:enumeration value="magnitudeNormal" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="component">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="profile">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="constant" />
                                                <xs:enumeration value="spatialDistribution" />
                                                <xs:enumeration value="temporalDistribution" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="value" type="vector" />
                                    <xs:element name="spacialDistribution" type="xs:string" /> <!-- HDF5 Dataset -->
                                    <xs:element name="temporalDistribution">
                                        <xs:complexType>
                                            <xs:sequence>
                                        <xs:element name="profile">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="directInput" />
                                                    <xs:enumeration value="file" />
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="directInput">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="piece" type="temporalVector" maxOccurs="unbounded" />
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="file" type="xs:string" /> <!-- HDF5 Dataset -->
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="magnitudeNormal">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="profile">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="constant" />
                                                <xs:enumeration value="spatialDistribution" />
                                                <xs:enumeration value="temporalDistribution" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="magnitude" type="xs:decimal" />
                                    <xs:element name="spacialDistribution" type="xs:string" /> <!-- HDF5 Dataset -->
                                    <xs:element name="temporalDistribution">
                                        <xs:complexType>
                                            <xs:sequence>
                                        <xs:element name="profile">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="directInput" />
                                                    <xs:enumeration value="file" />
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="directInput">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="piece" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="t" type="xs:decimal" />
                                                                <xs:element name="magnitude" type="xs:decimal" />
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="file" type="xs:string" /> <!-- HDF5 Dataset -->
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>

            </xs:element>
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
            <xs:element name="temperature" type="temperatureBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="flowRateInletType">
        <xs:sequence>
            <xs:element name="flowRate">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification" default="volumeFlowRate">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="volumeFlowRate" />
                                    <xs:enumeration value="massFlowRate" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="volumeFlowRate" type="xs:decimal" default="0" />
                        <xs:element name="massFlowRate" type="xs:decimal" default="0" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
            <xs:element name="temperature" type="temperatureBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="pressureInletType">
        <xs:sequence>
            <xs:element name="pressure" type="xs:decimal" default="0" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
            <xs:element name="temperature" type="temperatureBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="pressureOutletType">
        <xs:sequence>
            <xs:element name="totalPressure" type="xs:decimal" default="0" />
            <xs:element name="calculatedBackflow" type="xs:boolean" default="false" />
            <xs:element name="backflowTurbulence" type="turbulenceBoundaryConditionType" />
            <xs:element name="backflowTotalTemperature" type="xs:decimal" default="288.15" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ablInletType">
        <xs:sequence>
            <xs:element name="flowDirection" type="vector" />
            <xs:element name="groundNormalDirection" type="vector" />
            <xs:element name="referenceFlowSpeed" type="xs:decimal" />
            <xs:element name="referenceHeight" type="xs:decimal" />
            <xs:element name="surfaceRoughnessLength" type="xs:decimal" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="openChannelInletType">
        <xs:sequence>
            <xs:element name="volumeFlowRate" type="xs:decimal" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="openChannelOutletType">
        <xs:sequence>
            <xs:element name="meanVelocity" type="xs:decimal" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="freeStreamType">
        <xs:sequence>
            <xs:element name="streamVelocity" type="vector" />
            <xs:element name="pressure" type="xs:decimal" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="farFieldRiemannType">
        <xs:sequence>
            <xs:element name="flowDirection" type="vector" />
            <xs:element name="machNumber" type="xs:decimal" />
            <xs:element name="staticPressure" type="xs:decimal" default="101325" />
            <xs:element name="staticTemperature" type="xs:decimal" default="288.15" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="subsonicInflowType">
        <xs:sequence>
            <xs:element name="flowDirection" type="vector" />
            <xs:element name="totalPressure" type="xs:decimal" default="101325" />
            <xs:element name="totalTemperature" type="xs:decimal" default="288.15" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="subsonicOutflowType">
        <xs:sequence>
            <xs:element name="staticPressure" type="xs:decimal" default="101325" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="supersonicInflowType">
        <xs:sequence>
            <xs:element name="velocity" type="vector" />
            <xs:element name="staticPressure" type="xs:decimal" default="101325" />
            <xs:element name="staticTemperature" type="xs:decimal" default="288.15" />
            <xs:element name="turbulence" type="turbulenceBoundaryConditionType" />
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="wallType">
        <xs:sequence>
            <xs:element name="velocity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="type">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="noSlip" />
                                    <xs:enumeration value="slip" />
                                    <xs:enumeration value="movingWall" />
                                    <xs:enumeration value="atmosphericWall" />
                                    <xs:enumeration value="translationalMovingWall" />
                                    <xs:enumeration value="RotationalMovingWall" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="translationalMovingWall">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="velocity" type="vector" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="rotationalMovingWall">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="speed" type="xs:decimal" />
                                    <xs:element name="rotationAxisOrigin" type="vector" />
                                    <xs:element name="rotationAxisDirection" type="vector" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="temperature">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="type">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="adiabatic" />
                                    <xs:enumeration value="constantTemperature" />
                                    <xs:enumeration value="constantHeatFlux" />
                                    <xs:enumeration value="convection" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="temperature" type="xs:decimal" />
                        <xs:element name="heatFlux" type="xs:decimal" />
                        <xs:element name="heatTransferCoefficient" type="xs:decimal" />
                        <xs:element name="freeStreamTemperature" type="xs:decimal" />
