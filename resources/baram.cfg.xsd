                                                        </xs:simpleType>
                                                    </xs:element>
                                                    
                                                    <xs:element name="mrf" type="mrfZoneType" />

                                                    <xs:element name="porous" type="porousZoneType" />

                                                    <xs:element name="slidingMesh" type="slidingMeshZoneType" />

                                                    <xs:element name="actuatorDisk" type="actuatorDiskZoneType" />

                                                    <xs:element name="sourceTerms">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="mass">
                                                                </xs:element>
                                                                <xs:element name="energy"></xs:element>
                                                                <xs:element name="k"></xs:element>
                                                                <xs:element name="e"></xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="fixedValues">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="xVelocity">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="xs:decimal">
                                                                                <xs:attribute name="disabled" type="xs:boolean" default="true" />
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="yVelocity">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="xs:decimal">
                                                                                <xs:attribute name="disabled" type="xs:boolean" default="true" />
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="zVelocity">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="xs:decimal">
                                                                                <xs:attribute name="disabled" type="xs:boolean" default="true" />
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="temperature">
                                                                    <xs:complexType>
                                                                        <xs:simpleContent>
                                                                            <xs:extension base="xs:nonNegativeInteger">
                                                                                <xs:attribute name="disabled" type="xs:boolean" default="true" />
                                                                            </xs:extension>
                                                                        </xs:simpleContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>                                                </xs:sequence>
                                                <xs:attribute name="id" type="xs:positiveInteger" use="required" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="boundaryConditions">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="boundaryCondition" type="boundaryConditionType" minOccurs="0" maxOccurs="unbounded" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="numericalConditions" type="numericalConditionsType" />

                <xs:element name="monitors">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="forces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="force" type="forceMonitorType" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="points">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="point" type="pointMonitorType" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="surfaces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="surface" type="surfaceMonitorType" minOccurs="0" maxOccurs="unbounded" />
