                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="cellZones">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="region" minOccurs="0" maxOccurs="unbounded">
