                        <xs:enumeration value="patch" />
                        <xs:enumeration value="symmetryPlane" />
                        <xs:enumeration value="empty" />
                        <xs:enumeration value="wedge" />
                        <xs:enumeration value="cyclic" />
                        <xs:enumeration value="wall" />
                        <xs:enumeration value="processor" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="physicalType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="velocityInlet" />
                        <xs:enumeration value="flowRateInlet" />
                        <xs:enumeration value="pressureInlet" />
                        <xs:enumeration value="pressureOutlet" />
                        <xs:enumeration value="ablInlet" />
                        <xs:enumeration value="openChannelInlet" />
                        <xs:enumeration value="openChannelOutlet" />
                        <xs:enumeration value="outflow" />
                        <xs:enumeration value="freeStream" />
                        <xs:enumeration value="farFieldRiemann" />
                        <xs:enumeration value="subsonicInflow" />
                        <xs:enumeration value="subsonicOutflow" />
                        <xs:enumeration value="supersonicInflow" />
                        <xs:enumeration value="supersonicOutflow" />
                        <xs:enumeration value="wall" />
                        <xs:enumeration value="thermoCoupledWall" />
                        <xs:enumeration value="symmetry" />
                        <xs:enumeration value="interface" />
                        <xs:enumeration value="porousJump" />
                        <xs:enumeration value="fan" />
                        <xs:enumeration value="empty" />
                        <xs:enumeration value="cyclic" />
                        <xs:enumeration value="wedge" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="velocityInlet" type="velocityInletType" />

            <xs:element name="flowRateInlet" type="flowRateInletType" />

            <xs:element name="pressureInlet" type="pressureInletType" />

            <xs:element name="pressureOutlet" type="pressureOutletType" />

            <xs:element name="ablInlet" type="ablInletType" />

            <xs:element name="openChannelInlet" type="openChannelInletType" />

            <xs:element name="openChannelOutlet" type="openChannelOutletType" />

            <!-- no setting for "outflow" -->
