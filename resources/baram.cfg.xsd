                            <xs:element name="turbulenceModels" type="turbulenceModelsType" />
                            <xs:element name="speciesModels">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="off" />
                                        <xs:enumeration value="on" />
